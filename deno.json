{
	"$schema": "https://raw.githubusercontent.com/denoland/deno/348900b8b79f4a434cab4c74b3bc8d4d2fa8ee74/cli/schemas/config-file.v1.json",
	"compilerOptions": {
		"lib": [
			"dom",
			"dom.iterable",
			"dom.asynciterable",
			"deno.ns",
			"deno.unstable"
		],
		"noImplicitAny": false,
		"strict": false,
		"types": ["https://www.val.town/types/valtown.d.ts"]
	},
	"experimental": {
		"unstable-node-globals": true,
		"unstable-sloppy-imports": true,
		"unstable-temporal": true,
		"unstable-worker-options": true
	},
	"imports": {
		"@biomejs/biome": "npm:@biomejs/biome@^2.3.5"
	},
	"lint": {
		"files": {
			"include": ["deno:/https/esm.town/**/*"]
		},
		"rules": {
			"exclude": ["no-explicit-any", "no-import-prefix"]
		}
	},
	"lock": false,
	"nodeModulesDir": "auto"
}
